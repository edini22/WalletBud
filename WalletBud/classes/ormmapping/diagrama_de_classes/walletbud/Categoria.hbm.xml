<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="WB.walletbud.Categoria" table="Categoria" lazy="false">
		<id name="id_categoria" column="Id_categoria" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="name" column="Name" type="string" length="255" not-null="false" lazy="false"/>
		<property name="tipo" column="Tipo" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
