<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="WB.walletbud.Comentario" table="Comentario" lazy="false">
		<id name="id_comentario" column="Id_comentario" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="descrição" column="Descrição" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
