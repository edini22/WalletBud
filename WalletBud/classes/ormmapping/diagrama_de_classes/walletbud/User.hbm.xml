<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="WB.walletbud.User" table="`User`" lazy="false">
		<id name="id_user" column="Id_user" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="name" column="Name" type="string" length="255" not-null="false" lazy="false"/>
		<property name="password" column="Password" type="string" length="255" not-null="false" lazy="false"/>
		<property name="email" column="Email" type="string" length="255" not-null="false" lazy="false"/>
		<property name="saldo" column="Saldo" type="float" length="10" not-null="true" lazy="false"/>
		<property name="idioma" column="Idioma" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_UserTransacao" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="UserId_user" not-null="true"/>
			<one-to-many class="WB.walletbud.UserTransacao"/>
		</set>
	</class>
</hibernate-mapping>
