<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="WB.walletbud.UserTransacao" table="UserTransacao" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="userId_user" cascade="lock" column="UserId_user" class="WB.walletbud.User" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<many-to-one name="transacaoId_transacao" cascade="lock" column="TransacaoId_transacao" class="WB.walletbud.Transacao" not-null="true" lazy="proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
