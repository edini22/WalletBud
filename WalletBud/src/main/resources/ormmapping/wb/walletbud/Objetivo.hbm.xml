<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="wb.walletbud.Objetivo" table="Objetivo" lazy="false">
		<id name="id_objetivo" column="Id_objetivo" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="value" column="Value" type="float" length="10" not-null="true" lazy="false"/>
		<property name="name" column="Name" type="string" length="255" not-null="false" lazy="false"/>
		<property name="descrição" column="Descrição" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
