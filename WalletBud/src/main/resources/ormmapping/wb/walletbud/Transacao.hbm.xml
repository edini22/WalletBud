<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="wb.walletbud.Transacao" table="Transacao" lazy="false" discriminator-value="Transacao">
		<id name="id_transacao" column="Id_transacao" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<discriminator column="Discriminator" type="string"/>
		<property name="name" column="Name" type="string" length="255" not-null="false" lazy="false"/>
		<property name="value" column="Value" type="float" length="10" not-null="true" lazy="false"/>
		<property name="date" column="`Date`" type="date" not-null="false" lazy="false"/>
		<property name="descrição" column="Descrição" type="string" length="255" not-null="false" lazy="false"/>
		<property name="local" column="Local" type="string" length="255" not-null="false" lazy="false"/>
		<property name="tipo" column="Tipo" type="string" length="255" not-null="false" lazy="false">
			<meta attribute="field-description">
				receita ou despesa
			</meta>
		</property>
		<subclass name="wb.walletbud.Fixa" discriminator-value="Fixa" lazy="false">
			<property name="repetição" column="Repetição" type="integer" length="10" not-null="false" lazy="false"/>
			<property name="pago" column="Pago" type="boolean" not-null="false" lazy="false"/>
			<property name="data_pagamento" column="Data_pagamento" type="date" not-null="false" lazy="false"/>
			<property name="id_fixa" column="Id_fixa" type="integer" length="10" not-null="false" lazy="false"/>
		</subclass>
		<subclass name="wb.walletbud.Unica" discriminator-value="Unica" lazy="false">
		</subclass>
	</class>
</hibernate-mapping>
